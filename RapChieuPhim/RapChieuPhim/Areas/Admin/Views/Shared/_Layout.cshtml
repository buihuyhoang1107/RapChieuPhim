<!DOCTYPE html>

<html>
<head>
    @await Html.PartialAsync("_AdminHeadPartial.cshtml")
    @RenderSection("css", required: false)
</head>
<body>
    @await Html.PartialAsync("_AdminHeaderPartial.cshtml")
    @RenderBody()

    <div class="modal fade" id="form-modal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="addTaiKhoanLabel"> Thêm tài khoản</h4>
                    <button type="button" class="close" data-dismiss="modal">
                        <span>x</span>
                    </button>
                </div>
                <div class="modal-body">
                    @*layout của chức năng*@
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>

                    <button type="button" class="btn btn-primary" data-save="modal">Lưu</button>
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("_AdminFooterPartial.cshtml")
    @RenderSection("Scripts", required: false)
</body>
</html>
